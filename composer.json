{
    "name": "darkanakin41/video-bundle",
    "type": "symfony-bundle",
    "description": "Pierre LEJEUNE VideoBundle",
    "keywords": [
        "video",
        "symfony",
        "youtube",
        "bundle"
    ],
    "homepage": "https://github.com/darkanakin41/video-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Pierre LEJEUNE",
            "email": "darkanakin41@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "darkanakin41/core-bundle": "dev-master",
        "google/apiclient": "^2.4",
        "symfony/framework-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3",
        "doctrine/data-fixtures": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.4",
        "symfony/console": "^4.0",
        "symfony/test-pack": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Darkanakin41\\VideoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darkanakin41\\VideoBundle\\Tests\\": "tests/",
            "Darkanakin41\\CoreBundle\\Tests\\": "vendor/darkanakin41/core-bundle/tests/",
            "AppTestBundle\\": "tests/Fixtures/AppTestBundle/"
        }
    }
}
